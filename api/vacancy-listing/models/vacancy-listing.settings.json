{
  "kind": "collectionType",
  "collectionName": "vacancy_listings",
  "info": {
    "name": "vacancyListing",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "positionHeader": {
      "type": "string"
    },
    "vacancyOption": {
      "type": "enumeration",
      "enum": [
        "intern",
        "volunteerWork",
        "stockExchange",
        "administrationAssistance",
        "constructionRealEstate",
        "humanResources",
        "securityRescue",
        "military",
        "electronicsTelecomunication",
        "freelanceTemporary",
        "energyElectricity",
        "pharmacy",
        "financeAccounting",
        "infromationTechnology",
        "jurisprudenceJustice",
        "cultureArtEntertainemnt",
        "sports",
        "qualityControl",
        "agricutureEnvironmental",
        "medicineSocialCare",
        "mediaPublicRelations",
        "forestryWoodworkingMetalWorking",
        "marketingAdvertising",
        "productionIndustry",
        "seasonalWork",
        "technicalScience",
        "services",
        "transportLogistics",
        "tourismCateringHotels",
        "managment",
        "government",
        "others"
      ]
    },
    "contractType": {
      "type": "enumeration",
      "enum": [
        "temporarly",
        "fullTime",
        "partTime",
        "remoteFullTime",
        "remotePartTime",
        "other"
      ]
    },
    "workingTime": {
      "type": "enumeration",
      "enum": [
        "officeHours",
        "shifts",
        "flexible",
        "other"
      ]
    },
    "positionDescription": {
      "type": "richtext"
    },
    "positionRequirements": {
      "type": "richtext"
    },
    "positionBenefits": {
      "type": "richtext"
    },
    "currency": {
      "model": "currency"
    },
    "contactHours": {
      "type": "json"
    },
    "socialLinks": {
      "type": "json"
    },
    "monthlySalaryFrom": {
      "type": "integer"
    },
    "monthlySalaryTo": {
      "type": "integer"
    },
    "annualSalaryFrom": {
      "type": "integer"
    },
    "annualSalaryTo": {
      "type": "integer"
    },
    "hourlySalaryFrom": {
      "type": "decimal"
    },
    "hourlySalaryTo": {
      "type": "decimal"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "officeAddress": {
      "type": "string"
    },
    "contactTime": {
      "type": "enumeration",
      "enum": [
        "anyTime",
        "workingDays",
        "officeHours",
        "specificTimes"
      ]
    },
    "websiteLink": {
      "type": "string"
    },
    "listingGallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "videoLink": {
      "type": "string"
    },
    "tags": {
      "collection": "tag"
    },
    "bids": {
      "collection": "bid",
      "via": "vacancy_listing"
    },
    "popularity": {
      "type": "component",
      "repeatable": false,
      "component": "common.popularity"
    },
    "companyName": {
      "type": "string"
    },
    "enLanguages": {
      "type": "json"
    },
    "nativeLanguages": {
      "type": "json"
    },
    "country": {
      "model": "country"
    },
    "state": {
      "model": "state"
    },
    "city": {
      "model": "city"
    },
    "companyLogo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "insertDate": {
      "type": "date"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "bidOffer": {
      "type": "component",
      "repeatable": false,
      "component": "common.bidoffer"
    },
    "Applicants": {
      "type": "component",
      "repeatable": true,
      "component": "applicants.applicants"
    },
    "expiryDate": {
      "type": "date"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "KanBanColumns": {
      "type": "component",
      "repeatable": true,
      "component": "common.kan-ban-column"
    },
    "isPromotable": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "isPublished": {
      "type": "boolean",
      "default": true
    }
  }
}
